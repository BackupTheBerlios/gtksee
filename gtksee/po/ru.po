#: src/gtkseemenu.c:456
msgid "Command-line usage:\n"
msgstr "Синтаксис коммандной строки:\n"

#: src/gtkseemenu.c:457
msgid "    gtksee [-R[directory]] [-fis] [files...]\n"
msgstr "    gtksee [-R[директория]] [-fis] [файлы...]\n"

#: src/gtkseemenu.c:458
#, fuzzy
msgid "      -r  --  Use current directory as root\n"
msgstr "      -R  --  Использовать текущую директорию как корневую\n"

#: src/gtkseemenu.c:459
msgid "      -R<directory>  --  Use <directory> as root\n"
msgstr "      -R<директория>  --  Использовать <директория> как корневую\n"

#: src/gtkseemenu.c:460
msgid "      -f  --  Enable full-screen mode\n"
msgstr "      -f  --  Включить полноэкранный режим\n"

#: src/gtkseemenu.c:461
msgid "      -i  --  Enable fit-screen mode\n"
msgstr "      -i  --  Включить режим \"вместить в экран\"\n"

#: src/gtkseemenu.c:462
msgid "      -s  --  Enable slide-show mode\n"
msgstr "      -s  --  Включить режим показа слайдов\n"

#: src/gtkseemenu.c:463
msgid "      files... --  Launch gtksee viewer\n"
msgstr "      файлы... --  Запустить просмотрщик gtksee\n"

#: src/gtkseemenu.c:464
msgid ""
"      -h  --  Print help messages\n"
"\n"
msgstr ""
"      -h  --  Вывести справку\n"
"\n"

